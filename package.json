{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode=devDebug --appId 'com.reactnativetemplate.dev'",
    "ios": "react-native run-ios --scheme ReactNativeTemplateDebugDev --mode DebugDev",
    "start": "react-native start",
    "test": "jest",
    "lint:rules": "eslint . --cache",
    "lint:code-format": "prettier --check \"{src,__mocks__}/**/*.{js,json,md,ts,tsx,yml,yaml}\"",
    "lint:type-check": "tsc",
    "lint": "yarn lint:rules && yarn lint:code-format && yarn lint:type-check",
    "lint:fix": "yarn lint:rules --fix && yarn lint:code-format --write && yarn lint:type-check",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "npx pod-install",
    "postinstall": "npx patch-package",
    "ios-debug-develop": "react-native run-ios --scheme ReactNativeTemplateDebugDev --mode DebugDev",
    "ios-debug-staging": "react-native run-ios --scheme ReactNativeTemplateDebugStaging --mode DebugStaging",
    "ios-debug-production": "react-native run-ios --scheme ReactNativeTemplateDebugProd --mode DebugProd",
    "ios-release-develop": "react-native run-ios --scheme ReactNativeTemplateReleaseDev --mode ReleaseDev",
    "ios-release-staging": "react-native run-ios --scheme ReactNativeTemplateReleaseStaging --mode ReleaseStaging",
    "ios-release-production": "react-native run-ios --scheme ReactNativeTemplateReleaseProd --mode ReleaseProd",
    "android-debug-develop": "react-native run-android --mode=devDebug --appId 'com.reactnativetemplate.dev'",
    "android-debug-staging": "react-native run-android --mode=stagingDebug --appId 'com.reactnativetemplate.stg'",
    "android-debug-product": "react-native run-android --mode=prodDebug --appId 'com.reactnativetemplate.prod'",
    "android-release-develop": "react-native run-android --mode=devRelease --appId 'com.reactnativetemplate.dev'",
    "android-release-staging": "react-native run-android --mode=stagingRelease --appId 'com.reactnativetemplate.stg'",
    "android-release-product": "react-native run-android --mode=prodRelease --appId 'com.reactnativetemplate.prod'"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^4.5.7",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "@reduxjs/toolkit": "^2.6.0",
    "@shopify/flash-list": "^1.7.3",
    "@shopify/react-native-skia": "^2.0.7",
    "axios": "^1.8.1",
    "i18next": "^24.2.2",
    "intl-pluralrules": "^2.0.1",
    "moti": "^0.30.0",
    "react": "19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-native": "0.78.0",
    "react-native-config": "^1.5.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-keychain": "^9.2.3",
    "react-native-localize": "^3.4.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.9.1",
    "react-native-svg": "^15.11.2",
    "react-native-video": "^6.15.0",
    "react-native-webview": "^13.15.0",
    "react-native-youtube-iframe": "^2.3.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.6.3",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.0.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^7.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}
